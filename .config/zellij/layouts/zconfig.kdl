layout {
    default_tab_template {
        children
        pane size=2 borderless=true {
           plugin location="file:~/.config/zellij/plugins/statusbar.wasm"
        }
    }
    tab name="ZConfig" focus=true {
      pane borderless=true {
        cwd "~/.config/zellij/"
        command "nvim"
      }
    } 
}
